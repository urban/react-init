{
  "name": "{{name}}",
  "version": "0.0.1",
  "description": "{{description}}",
  "author": "{{author}} <{{email}}>",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "{{repository}}"
  },
  "readme": "README.md",
  "directories": {
    "lib": "./lib",
    "example": "./examples"
  },
  "files": [
    "./lib",
    "./README.md",
    "./LICENSE"
  ],
  "main": "./lib/index.jsx",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "gulp --gulpfile ./config/gulpfile.js --cwd .",
    "build-examples": "webpack --config ./config/webpack-examples.config.js --progress --profile --colors",
    "dev-examples": "webpack-dev-server --inline --content-base ./examples/ --config ./config/webpack-dev-examples.config.js --progress --colors"
  },
  "dependencies": {
    "react": "^0.12.x"
  },
  "devDependencies": {
    "browserify": "^6.3.3",
    "browserify-shim": "^3.8.0",
    "css-loader": "^0.9.0",
    "cssify": "^0.6.0",
    "envify": "^3.2.0",
    "file-loader": "^0.8.1",
    "gulp": "^3.8.10",
    "gulp-sourcemaps": "^1.2.8",
    "gulp-uglify": "^1.0.1",
    "jsx-loader": "^0.12.0",
    "merge-stream": "^0.1.6",
    "normalize.css": "^3.0.2",
    "object-assign": "^1.0.0",
    "react-tools": "^0.12.0",
    "reactify": "^0.17.1",
    "style-loader": "^0.8.1",
    "through2": "^0.6.3",
    "uglify-js": "^2.4.15",
    "url-loader": "^0.5.5",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0",
    "webpack": "^1.4.13",
    "webpack-dev-server": "^1.6.5"
  },
  "keywords": [
    "react",
    "react-ui",
    "react-element"
  ],
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "es6": true
        }
      ],
      "browserify-shim",
      [
        "envify",
        {
          "detect-globals": true
        }
      ],
      "cssify"
    ]
  },
  "browserify-shim": {
    "react": "global:React"
  }
}
