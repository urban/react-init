{
  "name": "{{name}}",
  "version": "1.0.0",
  "description": "{{description}}",
  "author": "{{author}} <{{email}}>",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "{{repository}}"
  },
  "readme": "README.md",
  "directories": {
    "lib": "./lib",
    "example": "./examples"
  },
  "files": [
    "./lib",
    "./README.md",
    "./LICENSE"
  ],
  "main": "./lib/{{name}}.js",
  "scripts": {
    "test": "jest",
    "release": "gulp --gulpfile ./config/gulpfile.js --cwd .",
    "build-examples": "webpack --config ./config/webpack-examples.config.js --progress --profile --colors",
    "dev-examples": "webpack-dev-server --inline --content-base ./examples/ --config ./config/webpack-dev-examples.config.js --progress --colors"
  },
  "jest": {
    "testDirectoryName": "test",
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": ["es6", "js", "jsx"],
    "moduleFileExtensions": ["js", "jsx", "json", "es6"]
  },
  "dependencies": {
    "babel": "^4.4.5",
    "react": "^0.12.2"
  },
  "devDependencies": {
    "babel-core": "^4.4.5",
    "babel-jest": "^4.0.0",
    "babel-loader": "^4.0.0",
    "babelify": "^5.0.3",
    "browserify": "^6.3.4",
    "browserify-shim": "^3.8.3",
    "envify": "^3.2.0",
    "file-loader": "^0.8.1",
    "gulp": "^3.8.11",
    "gulp-sourcemaps": "^1.3.0",
    "gulp-uglify": "^1.1.0",
    "jest": "^0.1.37",
    "jest-cli": "^0.2.2",
    "merge-stream": "^0.1.6",
    "through2": "^0.6.3",
    "uglify-js": "^2.4.16",
    "url-loader": "^0.5.5",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0",
    "webpack": "^1.5.3",
    "webpack-dev-server": "^1.7.0"
  },
  "keywords": [
    "react",
    "react-ui",
    "react-element"
  ],
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "experimental": true,
          "loose": "all"
        }
      ],
      "browserify-shim",
      [
        "envify",
        {
          "detect-globals": true
        }
      ]
    ]
  },
  "browserify-shim": {
    "react": "global:React"
  },
  "reactInitOptions": {
    "entryFile": "{{element}}.jsx",
    "exportVar": "{{element}}"
  }
}
